<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >
            <corners android:radius="5dp" />
            <stroke        android:width="2dp"        android:color="#0bc9e6"        />
            <gradient android:startColor="#00B8D4" android:endColor="#00E5FF" android:angle="90"/>
        </shape>
    </item>


    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >
            <corners android:radius="5dp" />
            <stroke        android:width="2dp"        android:color="#0bc9e6"        />
            <gradient android:startColor="#00E5FF" android:endColor="#00B8D4" android:angle="90"/>
        </shape>
    </item>

</selector>

